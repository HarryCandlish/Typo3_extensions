{
    "name": "ocular-nz/blank",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "description": "Base typo3 site",
    "authors": [
        {
            "name": "Ocular",
            "email": "info@ocular.co.nz"
        }
    ],
    "repositories": {
        "slider": {
            "type": "vcs",
            "url": "git@github.com:ocular-nz/ocularslider.git"
        },
        "events": {
            "type": "vcs",
            "url": "git@github.com:ocular-nz/events.git"
        },
        "fix404": {
            "type": "vcs",
            "url": "git@github.com:ocular-nz/fix404.git"
        }
    },
    "require": {
        "bk2k/bootstrap-package": "^10.0.0",
        "friendsoftypo3/extension-builder": "^9.10",
        "georgringer/news": "7.0.8",
        "helhum/typo3-console": "^5.5.5",
        "ocular-nz/events": "dev-master",
        "ocular-nz/fix404": "dev-master",
        "ocular-nz/ocularslider": "dev-master",
        "plusb/pb_social": "^1.4",
        "t3/dce": "^2.0",
        "typo3/cms-about": "^9.5",
        "typo3/cms-adminpanel": "^9.5",
        "typo3/cms-belog": "^9.5",
        "typo3/cms-beuser": "^9.5",
        "typo3/cms-felogin": "^9.5",
        "typo3/cms-form": "^9.5",
        "typo3/cms-impexp": "^9.5",
        "typo3/cms-info": "^9.5",
        "typo3/cms-redirects": "^9.5",
        "typo3/cms-reports": "^9.5",
        "typo3/cms-seo": "^9.5",
        "typo3/cms-setup": "^9.5",
        "typo3/cms-sys-note": "^9.5",
        "typo3/cms-t3editor": "^9.5",
        "typo3/cms-tstemplate": "^9.5",
        "typo3/cms-viewpage": "^9.5",
        "typo3/minimal": "^9.5"
    },
    "require-dev": {
        "codeception/codeception": "^2.5.1"
    },
    "autoload": {
        "psr-4": {
            "Ocular\\VersionControlled\\": "public/typo3conf/ext/version_controlled/Classes/",
            "MyVendor\\StoreInventory\\": "public/typo3conf/ext/store_inventory/Classes/",
            "Product\\Productview\\": "public/typo3conf/ext/productview/Classes/",
            "EditForm\\Editform\\":"public/typo3conf/ext/editform/Classes/"

        }
    },
    "config": {
        "sort-packages": true,
        "bin-dir": "vendor/bin",
        "vendor-dir": "vendor"
    },
    "minimum-stability": "stable"
}
